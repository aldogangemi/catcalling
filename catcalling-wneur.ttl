###  http://www.ontologydesignpatterns.org/ont/cog/catcalling.owl#CognitiveCluster
:CognitiveCluster rdf:type owl:NamedIndividual ,
                           :FrameCluster ;
                  :coreRegion neur:ACC ,
                              neur:PFC ;
                  :hasComponent :AttentionAwareness ,
                                :CognitiveAssessment ,
                                :ComprehensionAssessment ,
                                :ComprehensionBarrier ,
                                :CuriosityShift ,
                                :ExperientialLearning ,
                                :MentalProcessing ,
                                :MetaAwareness ,
                                :PatternRecognition ,
                                :RepetitionAwareness ,
                                :ResponseEvolution ,
                                :SituationalAwareness ,
                                :StrategicPlanning ,
                                :TechnicalCuriosity ,
                                :TemporalShift ;
                  :supportingRegion neur:Amygdala ,
                                    neur:FrontalEyeField ,
                                    neur:Hypothalamus ,
                                    neur:IFG ,
                                    neur:PosteriorDorsolateralPFC ,
                                    neur:STG ,
                                    neur:TemporoparietalJunction ;
                  rdfs:comment "Neural patterns derived from subjects E2, E4, E9, E10, E13, C2, C3, C4, C5, C15, C17, C18 showing cognitive assessment responses. Core regions (67% consistency): PFC, ACC. Supporting regions: Language Circuit, Amygdala"@en ,
                               "Neural patterns derived from subjects E2, E4, E9, E10, E13, C2, C3, C4, C5, C15, C17, C18 showing cognitive assessment responses. Core regions (67% consistency): PFC, ACC. Supporting regions: Language Circuit, Amygdala, ENHANCED: TPJ, Frontal Eye Field, Posterior DLPFC"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CognitiveCluster ;
   owl:annotatedProperty :coreRegion ;
   owl:annotatedTarget neur:PFC ;
   :consistency 0.67
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CognitiveCluster ;
   owl:annotatedProperty :supportingRegion ;
   owl:annotatedTarget neur:FrontalEyeField ;
   :consistency 0.08
 ] .

[ rdf:type owl:Axiom ;
